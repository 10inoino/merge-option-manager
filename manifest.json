{
    "name": "chrome-extension-demo",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Chrome Extension demo",
    "content_scripts": [{
        "matches": ["https://github.com/*/*/pull/*"],
        "js": [
            "content.js"
        ]
    }],
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "browser_action": {
        "default_title": "merge-option-manager",
        "default_popup": "popup.html"
    },
    "permissions": ["tabs","storage"]
}
