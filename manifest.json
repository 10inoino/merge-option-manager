{
    "name": "chrome-extension-demo",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Chrome Extension demo",
    "content_scripts": [{
        "matches": ["https://github.com/*/*/pull/*"],
        "js": [
            "content.js"
        ]
    }],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "acid-32.png"
    },
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "permissions": [
        "storage",
        "webNavigation",
        "tabs"
    ]
}
